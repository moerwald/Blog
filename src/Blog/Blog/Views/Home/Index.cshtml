@model IEnumerable<Post>


<h2>Blogs</h2>

<div>
    <ul>
        @foreach (var post in Model)
        {
            <li>
                <a asp-controller="Home" asp-action="Post" asp-route-id="@post.Id">@post.Title</a> -
                <a asp-controller="Home" asp-action="Edit" asp-route-id="@post.Id">Edit</a>
            </li>
        }
    </ul>
</div>
